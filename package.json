{
  "name": "bulk-publish",
  "version": "0.0.0",
  "author": "Abhinav Gupta @abhinav-from-contentstack",
  "bin": {
    "bulk-publish": "./bin/run"
  },
  "bugs": "https://github.com/abhinav-from-contentstack/bulk-publish/issues",
  "dependencies": {
    "@oclif/command": "^1.6.1",
    "@oclif/config": "^1.15.1",
    "@oclif/plugin-help": "^3.1.0",
    "bluebird": "^3.7.2",
    "nock": "^13.0.0",
    "request": "^2.88.2",
    "winston": "^3.3.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.6",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/abhinav-from-contentstack/bulk-publish",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "bulk-publish",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "abhinav-from-contentstack/bulk-publish",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "node copyLogs.js && nyc --reporter lcov mocha \"test/**/*.test.js\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
